      <record>
        <tri_uid>3020328385cd979b3ce6643055352115</tri_uid>
        <tri_title>TechCheckReview</tri_title>
        <tri_description></tri_description>
        <pro_uid>9810584265cb9d56a295984036422203</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$items = [];
$i = 1;
@%NotPassedTechCheckCount = 0;
foreach(@=ItemsList as $item)
{
	$newItem = $item;
	if($item['purchase'] === '1'
	   && $item['tech_check'] === 'yes'
	  && $item['tech_check_res'] !== 'yes' )
	{
		@%NotPassedTechCheckCount++;
		$newItem['purchase'] = "0";
		$newItem['purchase_label'] = "0";
		if($item['tender_requested'] == '1')
		{
			$newItem['tender_requested'] = "0";
			$newItem['tender_requested_label'] = "0";
			@%OffersForTenderCount--;
		}		
	}
	$items[$i] = $newItem;
	$i++;
}

@=ItemsList = $items;

@@created_at = date('Y-m-d h:i:s');
@%del_index = @%INDEX;
$task = PMFGetTaskName(@@TASK,'en');
$itemsLeft = false;
foreach(@=ItemsList as $item)
{
	if($item['purchase'] === '0') 
	{
		$itemsLeft = true;
	}
}

//for report table
if(@%OffersForTenderCount > 0){
	@@task_name = "Тендерная проверка руководителем отдела";
}else if(@%NotPassedTechCheckCount >0){
	@@task_name = "Ознакомление с результатми проверки";	
}else if(@%NotPassedTechCheckCount  == 0 && $itemsLeft){
	@@task_name = "Поиск новых ценовых  предложений";	
}else{
	@@task_name = "Закупка у поставщика";
}
]]></tri_webbot>
        <tri_param></tri_param>
      </record>
      <record>
        <tri_uid>4906370505cbcbfdcc77241002887671</tri_uid>
        <tri_title>ReviewNewOffers</tri_title>
        <tri_description></tri_description>
        <pro_uid>9810584265cb9d56a295984036422203</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$items = @=ItemsList;
$count = 0;
$tenderCount = 0;
$techCount = 0;
$itemsCount = count($items);
$offer_types = ['new','analogue','correction'];
for($i = 1; $i <= $itemsCount; $i++)
{
	$item = $items[$i];
	$offer = $item['offer_type'];
	$tender = $item['tender_requested'];
	$purchase = $item['purchase'];
	if(in_array($offer,$offer_types))
	{
		$count++;
	}
	
	if($tender === '1' && $purchase === '1')
	{		
		$tenderCount++;
	}
	
	$techCheck = $item['tech_check'];
	if($techCheck === 'yes')
	{
		$techCount++;
	}
}

@%NewOffersCount = $count;
@%OffersForTenderCount = $tenderCount;
@%OffersForTechCheckCount = $techCount;]]></tri_webbot>
        <tri_param></tri_param>
      </record>
      <record>
        <tri_uid>5013681825ccb4f70845607013808844</tri_uid>
        <tri_title>setDataForReportTable</tri_title>
        <tri_description></tri_description>
        <pro_uid>9810584265cb9d56a295984036422203</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[@@created_at = date('Y-m-d h:i:s');
@%del_index = @%INDEX;
$task = PMFGetTaskName(@@TASK,'en');
$itemsLeft = false;
$i = 1;
foreach(@=ItemsList as $item)
{
	if($item['purchase'] === '0') 
	{
		$itemsLeft = true;
	}
}
if($task == "Поиск новых ценовых предложений"){
	if(@%OffersForTechCheckCount > 0){
		@@task_name = "Проверка технических параметров";
	}else{
		if(@%OffersForTenderCount > 0){
			@@task_name = "Тендерная проверка руководителем отдела";
		}else if(@%NotPassedTechCheckCount >0){
			@@task_name = "Ознакомление с результатми проверки";	
		}else if(@%NotPassedTechCheckCount  == 0 && $itemsLeft){
			@@task_name = "Поиск новых ценовых  предложений";	
		}else{
			@@task_name = "Закупка у поставщика";
		}
	}
}else if($task == 'Тендерная проверка руководителем отдела'){
	if(@=TopMgrApproveRequired){
			@@task_name = "Тендерная проверка руководителем предприятия";
		}else{
			@@task_name = "Ознакомление с результатми проверки";
		}
}else if($task == 'Ознакомление с результатми проверки'){
	 if($itemsLeft){
			@@task_name = "Поиск новых ценовых предложений";	
	 }else{
		    @@task_name = "Закупка у поставщика";
	 }
}else if($task == 'Тендерная проверка руководителем предприятия'){
    @@task_name = "Ознакомление с результатми проверки";
}

]]></tri_webbot>
