        <dyn_content><![CDATA[{"name":"AcknowledgeTenderResult","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"5611873895cc06d034875f2003578373","name":"AcknowledgeTenderResult","description":"","mode":"edit","script":{"type":"js","code":"$('.title-column').css('white-space','pre-line');\n\nconst selectedRowColor = \"#bebebe\";\nconst mouseOverColor = \"#f0f7fd\";\nconst mouseOverPassedColor = \"#dff0d8\";\nconst mouseOverErrorColor = \"#fcf2f2\";\nconst code_idx = 1;\nconst item_idx = 2;\nconst unit_idx = 3;\nconst quantity_idx = 4\nconst price_idx = 5;\nconst suppl_idx = 6;\nconst mgr_tender_passed_idx = 7;\nconst mgr_tender_just_idx = 8;\nconst top_tender_passed_idx = 9;\nconst top_tender_just_idx = 10;\nconst gridId = 'IngoingItemsList';\nconst formId = '7450183285bd53975abb8c0015043457';\nlet rows  = $(\"#\"+gridId).find(\"div.pmdynaform-grid-row\");\nlet currentRow = -1;\nrows.click(clickHandler);\nrows.mouseover(mouseoverHandler);\nrows.mouseout(mouseoutHandler);\nclearSourceItemFields();\n\nlet rowsNum = $(\"#\"+gridId).getNumberRows();\nfor (var i=1; i <= rowsNum; i++)\n{\n  var r = $(\"#\"+gridId).getControl(i);\n  console.log(r);\n  $(\"#\"+gridId).getControl(i, code_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, item_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, unit_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, quantity_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, price_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, suppl_idx).attr(\"readOnly\", true);    \n  $(\"#\"+gridId).getControl(i, top_tender_passed_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, mgr_tender_passed_idx).attr(\"readOnly\", true);\n}\n\nfunction mouseoverHandler() \n{ \n  document.body.style.cursor = 'pointer';\n  let firstFieldInRow = $(this).find(\"input.form-control\").first();\n  let rowNum = firstFieldInRow.prop(\"id\").match(\/\\]\\[(\\d+)\\]\\[\/)[1];\n  if(rowNum == currentRow)\n  {\n    $(this).css(\"background-color\", selectedRowColor);\n    return;\n  }\n  let tender =  $(\"#\"+gridId).getValue(rowNum,tender_passed_idx);\n  if(tender === 'yes')\n  {\n   $(this).css(\"background-color\", mouseOverPassedColor); \n  }\n  else if(tender === 'no')\n  {\n    $(this).css(\"background-color\", mouseOverErrorColor); \n  } \n  else\n  {\n    $(this).css(\"background-color\", mouseOverColor); \n  }\n}\nfunction mouseoutHandler() \n{\n  document.body.style.cursor = 'default';\n  let firstFieldInRow = $(this).find(\"input.form-control\").first();\n  let rowNum = firstFieldInRow.prop(\"id\").match(\/\\]\\[(\\d+)\\]\\[\/)[1];\n  if(rowNum == currentRow)\n  {\n    $(this).css(\"background-color\", selectedRowColor);    \n  }\n  else\n  {\n    $(this).css(\"background-color\", '');\n  }  \n}\n\n\nfunction clickHandler()\n{  \n  clearSourceItemFields();\n  let firstFieldInRow = $(this).find(\"input.form-control\").first();\n  let rowNum = firstFieldInRow.prop(\"id\").match(\/\\]\\[(\\d+)\\]\\[\/)[1];\n  $($('#'+gridId).find(\"div.pmdynaform-grid-row\")).css(\"background-color\", \"\");\n  $($('#'+gridId).find(\"div.pmdynaform-grid-row\")[rowNum-1]).css(\"background-color\", selectedRowColor);\n  currentRow = rowNum;\n  $(\"#item_code_form\").setValue($(\"#\"+gridId).getText(rowNum,code_idx));\t  \n  $(\"#item_title_form\").setValue($(\"#\"+gridId).getText(rowNum,item_idx));\n  $(\"#item_supplier_form\").setValue($(\"#\"+gridId).getText(rowNum,suppl_idx));\n  $(\"#item_price_form\").setValue($(\"#\"+gridId).getText(rowNum,price_idx));\n  $(\"#item_quantity_form\").setValue($(\"#\"+gridId).getText(rowNum,quantity_idx));\n  $(\"#item_unit_form\").setValue($(\"#\"+gridId).getText(rowNum,unit_idx));    \n  $(\"#mgr_check_res_form\").setValue($(\"#\"+gridId).getValue(rowNum,mgr_tender_passed_idx));\n  let mgrCmt = $(\"#\"+gridId).getValue(rowNum,mgr_tender_just_idx);\n  $(\"#mgr_check_just_form\").setValue(mgrCm