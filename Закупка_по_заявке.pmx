      <record>
        <tri_uid>1949522335cbdf569c6ea92063271261</tri_uid>
        <tri_title>PrepareDataForSupplier</tri_title>
        <tri_description></tri_description>
        <pro_uid>9810584265cb9d56a295984036422203</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[try{
	@=CurrentItems = array();
	$supplierIdx = @%SuppliersToBeProceed;
	$currentItems = array();
	$suppliers = @=SuppliersList;
	$supplier =  $suppliers[$supplierIdx];
	@@current_supplier_code = $supplier;
	
	//filling items of curent supplier
	$idx = 1;
	$supplierIsSet = false;
	
	foreach(@=ItemsList as $item)
	{
		if(!$item['purchase']) continue;		
		if($item['supplier_code'] == $supplier)
		{
			if(!$supplierIsSet)
			{
				@@CurrentSupplier = $item['supplier'];
				$supplierIsSet = true;
			}
			$currentItems[$idx] = $item;
			$idx++;
		}
	}
	@=CurrentItems = $currentItems;
	@%SuppliersToBeProceed--;
	
	@@task_name = "Закупка у поставщика";
	@@NextTask = "Проверка необходимости заключения договора";
	@%NextDelIndex = 1;
	@@NextCreatedAt = date('Y-m-d h:i:s');
	$userUID = @@PerformerId;
	$aUser = PMFInformationUser($userUID);
	@@performer_name = $aUser['firstname'].' '.$aUser['lastname'];
	@%items_list_corrected = 0;   
	
}catch(Exception $e){
	echo 'Caught exception: ',  $e->getMessage(), "\n";
	exit(0);
}]]></tri_webbot>
        <tri_param></tri_param>
      </record>
      <record>
        <tri_uid>2286957145cbde723339ff8008753905</tri_uid>
        <tri_title>PrepareDataForProcessing</tri_title>
        <tri_description></tri_description>
        <pro_uid>9810584265cb9d56a295984036422203</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[@=ItemsLeft = false;
try{
	@%SuppliersToBeProceed = 0;
	$items = [];
	$idx = 1;
	foreach(@=ItemsList as $item)
	{
		$newItem = $item;
		$purchase = $item['purchase'];
		$tenderRequired =  $item['tender_required'];
		$tenderPassed =  $item['tender_passed'];
		if($tenderRequired === "1" && $tenderPassed !== 'yes')
		{
			$purchase = "0";
		}
		
		if($purchase === "1")
		{
			$offer = $item['offer_type'];
			if($offer !== 'none')
			{
				$newItem['price'] = $item['analogue_price'];
				if($offer === 'new' || $offer === 'analogue')
				{
					$newItem['supplier'] = $item['analogue_supplier'];
					$newItem['supplier_code'] = $item['new_supplier_code'];
				}
				if($offer === 'analogue')
				{
					$newItem['title'] = $item['analogue_title'];
				}
			}
		}
		else
		{
			@=ItemsLeft = true;
		}
		$items[$idx] = $newItem;
		$idx++;
	}
	
	@=ItemsList = $items;	
	$suppliers = [];
	$i = 0;
	foreach(@=ItemsList as $item)
	{	
		$supplier = $item['supplier_code'];
		$purchase = $item['purchase'];
		if($purchase === "1" && !in_array($supplier, $suppliers))
		{
			$i++;
			@=SuppliersList[$i] = $supplier;
			array_push($suppliers, $supplier);
		}
	}
	@%SuppliersToBeProceed = $i;
}catch(Exception $e){
	 echo 'Caught exception: ',  $e->getMessage(), "\n";
	 exit(0);
}]]></tri_webbot>
