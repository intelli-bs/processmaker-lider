        <dyn_content><![CDATA[{"name":"ReviewRequest","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"6057593775cb9d81a2bb6a9074070966","name":"ReviewRequest","description":"","mode":"edit","script":{"type":"js","code":"$('.title-column').css('white-space','pre-line');\n$('a.form-control').css('color','#2a6496');\n$('a.form-control').css('text-decoration','underline');\nconst normalHover = '#ededed';\nconst normal = '#fff';\nconst gridId = 'ItemsList';\nconst code_idx = 1;\nconst price_idx = 5;\nconst supplier_idx = 6;\nconst tender_idx = 7;\nconst mgr_passed_idx = 8;\nconst top_passed_idx = 9;\nlet rows = $(\"#\"+gridId).find(\"div.pmdynaform-grid-row\");\nlet rowsNum = $(\"#\"+gridId).getNumberRows();\nlet shouldFindNewOffer = false;\nfor (var i=1; i <= rowsNum; i++)\n{\n  $(\"#\"+gridId).setValue('none',i, mgr_passed_idx);\n  $(\"#\"+gridId).setValue('none',i, top_passed_idx);\n  let code = $(\"#\"+gridId).getValue(i,code_idx);\n  let supplier = $(\"#\"+gridId).getValue(i,supplier_idx);\n  let price = $(\"#\"+gridId).getValue(i,price_idx);\n  if((code == null || code.trim() == '')\n    || (supplier == null || supplier.trim() == '')\n    || (price == null || price.trim() == ''))\n  {\n     shouldFindNewOffer = true;\n  }\n  let tender = $(\"#\"+gridId).getValue(i,tender_idx);\n  \n}\n\nif($('#NewItemsSearchRequired').getValue() == 1)\n{\n  shouldFindNewOffer = true;\n}\n\nif(shouldFindNewOffer)\n{\n  $('#SearchForAnalogues').setValue(1);\n  $(\"#SearchForAnalogues\").getControl().attr(\"disabled\", true);\n}\n\nif($('#TenderRequired').getValue() == 1)\n{\n  $('#TenderRequested').setValue(1);\n  $(\"#TenderRequested\").getControl().attr(\"disabled\", true);\n}\n\nlet requestComment = $('#RequestComment').getValue();\nif(requestComment == null || requestComment.trim() == '')\n{ $('#RequestComment').hide(); }\n\n$($(\"#\"+gridId).find(\"div.pmdynaform-grid-row\")).hover(function(){\n     $(this).css(\"background-color\", normalHover);\n     $($(\"#\"+gridId).find(\"div.pmdynaform-grid-row:eq(\"+($(\"div.pmdynaform-grid-row\").index(this))+\") .form-control\")).css(\"background-color\", normalHover);\n  }, function(){\n     $(this).css(\"background-color\", normal);\n      $($(\"#\"+gridId).find(\"div.pmdynaform-grid-row:eq(\"+($(\"div.pmdynaform-grid-row\").index(this))+\") .form-control\")).css(\"background-color\", normal);\n\n});"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"\u041e\u0437\u043d\u0430\u043a\u043e\u043c\u043b\u0435\u043d\u0438\u0435 \u0441 \u0437\u0430\u044f\u0432\u043a\u043e\u0439","colSpan":3},{"type":"text","variable":"RequestId","var_uid":"4554802575bcf2666055c58026143929","dataType":"string","protectedValue":false,"id":"RequestId","name":"RequestId","label":"\u041d\u043e\u043c\u0435\u0440 \u0437\u0430\u043a\u0430\u0437\u0430","defaultValue":"","placeholder":"","hint":"","required":false,"requiredFieldErrorMessage":"","textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"view","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"RequestId","colSpan":3},{"type":"text","variable":"RequestPriorityText","var_uid":"3680986735cb9d820b3c656058653956","dataType":"string","protectedValue":false,"id":"RequestPriorityText","name":"RequestPriorityText","label":"\u041f\u0440\u0438\u043e\u0440\u0438\u0442\u0435\u0442","defaultValue":"","placeholder":"","hint":"","required":false,"requiredFieldErrorMessage":"","textTransform":"none","validate":"","validateMessage":"","maxLength":"1000","formula":"hhhhhhhhhhh","mode":"view","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"RequestPriorityText","colSpan":3},{"type":"link","id":"link0000000001","name":"link0000000001","label":"","value":"\u0438\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0438\u044f \u043f\u043e \u0440\u0430\u0431\u043e\u0442\u