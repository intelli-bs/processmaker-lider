        <dyn_content><![CDATA[{"name":"TenderTop","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"8469745885cbcba3144f9e7038527206","name":"TenderTop","description":"","mode":"edit","script":{"type":"js","code":"$('.title-column').css('white-space','pre-line');\nclearSourceItemFields();\nclearAnalogueFields();\nconst formId = '8469745885cbcba3144f9e7038527206';\nconst selectedRowColor = \"#bebebe\";\nconst mouseOverColor = \"#ededed\";\nconst code_idx = 1;\nconst item_idx = 2;\nconst suppl_idx = 3;\nconst price_idx = 4;\nconst quant_idx = 5;\nconst unit_idx = 6;\nconst ang_idx = 7;\nconst ang_suppl_idx = 8;\nconst ang_price_idx = 9;\nconst tend_doc_idx = 10;\nconst top_mrg_check_res_idx = 11;\nconst top_mrg_check_cmt_idx = 12;\nconst mrg_check_res_idx = 13;\nconst mrg_check_cmt_idx = 14;\nconst offer_idx = 15;\nconst tech_check_idx = 16;\nconst tech_check_res_idx = 17;\nconst tech_check_cmt_idx = 18;\nconst tender_passed_idx = 19;\nconst tech_doc_url_idx = 21;\nconst purchase_idx = 23;\nconst tender_required_idx = 24;\nconst tender_requested_idx = 25;\nconst new_suppl_code_idx = 25;\nconst gridId  = \"ItemsList\"; //set to the ID of the grid\n//set click event handler when the DynaForm loads for any existing rows in the grid:\nlet analogueRows = $(\"#\"+gridId).find(\"div.pmdynaform-grid-row\");\nlet currentAngRow = 0;\nanalogueRows.click(clickHandler);\nanalogueRows.mouseover(mouseoverHandler);\nanalogueRows.mouseout(mouseoutHandler);\n$(\"#save_form_button\").find(\"button\").click(saveFormClick);\n$(\".rowIndex\").hide();\n$(\"#ang_tech_check_cmt_form\").hide();\n$(\"#ang_tech_url_form\").hide();\n\nlet rowsNumAng = $(\"#\"+gridId).getNumberRows();\nfor (var i=1; i <= rowsNumAng; i++)\n{\n  let mgrCheckRes = $(\"#\"+gridId).getValue(i, top_mrg_check_res_idx);\n  if(mgrCheckRes == null || mgrCheckRes == '')\n  {\n    $(\"#\"+gridId).setValue('none',i, top_mrg_check_res_idx);\n  } \n  \n  let pdMgrCheckRes = $(\"#\"+gridId).getValue(i, mrg_check_res_idx); \n  console.log(pdMgrCheckRes);\n  if(pdMgrCheckRes !== 'passed')\n  {\n    $(analogueRows[i-1]).hide();\n  }\n}\n\nfor (var i=1; i <= rowsNumAng; i++)\n{\n  $(\"#\"+gridId).getControl(i, code_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, item_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, suppl_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, price_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, ang_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, ang_price_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, ang_suppl_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, ang_idx).attr(\"readOnly\", true);\n}\n\n$(\"#\"+formId).setOnSubmit(\n  function() \n  {\n    let isValid = checkInput();\n    if(!isValid)\n    {\n      $('#invalidInput').modal('show');\n    }\n    else\n    {\n      for(var i=1; i <= rowsNumAng; i++)\n      {\n        let topMgrCheckRes = $(\"#\"+gridId).getValue(i, top_mrg_check_res_idx);\n  \t    if(topMgrCheckRes === 'passed')\n        {\n          $(\"#\"+gridId).setValue('yes',i, tender_passed_idx);\n        }\n        else if(topMgrCheckRes ==='failed')\n        {          \n          $(\"#\"+gridId).setValue('no',i, tender_passed_idx);\n        } \n      }\n      \n    }\n    return isValid;\n  });\n/*\n * Back to top button\n */\n\n$(\"#back-to-top\").css( \"display\" , \"none\");\n$(\"#back-to-top\").css( \"position\" , \"fixed\");\n$(\"#back-to-top\").css( \"bottom\" , \"42px\");\n$(\"#back-to-top\").css( \"right\" , \"25px\");\n$(\"#back-to-top\").css( \"text-align\" , \"center\");\n$(\"#back-to-top\").css( \"height\" , \"32px\");\n$(\"#back-to-top\").css( \"width\" , \"32px\");\n$(\"#back-to-top\").css( \"cursor\" , \"pointer\");\n$(\"#back-to-top\").css( \"border\" , \"0\");\n$(\"#back-to-top\").css( \"border-radius\" , \"2px\");\n$(\"#back-to-top\").css( \"text-decoration\" , \"none\");\n$(\"#back-to-top\").css( \"padding-left\" , \"9px\");\n$(\"#back-to-top\").click(function(e) {\n            e.preventDefault();\n            $('html, body').animate({\n                scrollTop: 0\n            }, 100);\n       });\n$(window).scroll(function () {\n            if ($(this).scrollTop() > 500) {\n                $('#back-to-top').fadeIn();\n            } else {\n                $('#back-to-top').fadeOut();\n            }\n        });\nfunction checkInput()\n{\n  var validate = true;      \n    for (var i=1; i <= rowsNumAng; i++) \n    {\n      $(\"#\"+gridId).getControl(i,top_mrg_check_res_idx).css(\"border\",\"\");\n      $(\"#\"+gridId).getControl(i,top_mrg_check_res_idx).css(\"border\",\"\"); \n     let pdMgrCheckRes = $(\"#\"+gridId).getValue(i, mrg_check_res_idx); \n     if(pdMgrCheckRes === 'passed')\n     {\n       let checkRes = $(\"#\"+gridId).getValue(i,top_mrg_check_res_idx);\n       if(checkRes === 'none')\n       {\n         validate = false;  \n         $(\"#\"+gridId).getControl(i,top_mrg_check_res_idx).css(\"border\",\"red solid 1px\");          \n       }        \n      }          \n    }\n    return validate;\n}\n\nfunction saveFormClick()\n{\n  $(\"#\" + formId).saveForm();\n  $('#saved').modal('show');\n}\n\nfunction mouseoverHandler() \n{\n  document.body.style.cursor = 'pointer';\n  checkInput();\n  $(this).css(\"background-color\", mouseOverColor);\n}//set row highlighting\nfunction mouseoutHandler() \n{\n  document.body.style.cursor = 'default';\n  checkInput();\n  let firstFieldInRow = $(this).find(\"input.form-control\").first();\n  let rowNum = firstFieldInRow.prop(\"id\").match(/\\]\\[(\\d+)\\]\\[/)[1];\n  if(rowNum != currentAngRow)\n  {\n    $(this).css(\"background-color\", '');\n  }//reset to orginal color\n  else\n  {\n    $(this).css(\"background-color\", selectedRowColor);\n  }\n}\n\n//click event handler function to display an alert if the field is left empty:\nfunction clickHandler()\n{  \n  clearAnalogueFields();\n  clearSourceItemFields();\n  let firstFieldInRow = $(this).find(\"input.form-control\").first();\n  let rowNum = firstFieldInRow.prop(\"id\").match(/\\]\\[(\\d+)\\]\\[/)[1];\n  $($('#'+gridId).find(\"div.pmdynaform-grid-row\")).css(\"background-color\", \"\");\n  $($('#'+gridId).find(\"div.pmdynaform-grid-row\")[rowNum-1]).css(\"background-color\", selectedRowColor);\n  currentAngRow = rowNum;\n  $(\"#item_code_form\").setValue($(\"#\"+gridId).getText(rowNum,code_idx));\t  \n  $(\"#item_title_form\").setValue($(\"#\"+gridId).getText(rowNum,item_idx));\n  $(\"#item_supplier_form\").setValue($(\"#\"+gridId).getText(rowNum,suppl_idx));\n  $(\"#item_price_form\").setValue($(\"#\"+gridId).getText(rowNum,price_idx));\n  $(\"#item_quantity_form\").setValue($(\"#\"+gridId).getText(rowNum,quant_idx));\n  $(\"#item_unit_form\").setValue($(\"#\"+gridId).getText(rowNum,unit_idx));  \n  let offer = $(\"#\"+gridId).getValue(rowNum,offer_idx);\n  if(offer === ''){ offer = 'none'};\n  $(\"#offer_type_form\").setValue(offer);\n  if(offer !== 'none')\n  {\n    if(offer == 'analogue')\n    {\n      $(\"#analogue_title_form\").show();\n      $(\"#analogue_supplier_form\").setLabel('Поставщик аналога');\n      $(\"#analogue_price_form\").setLabel('Цена аналога');\n      $(\"#analogue_title_form\").setValue($(\"#\"+gridId).getText(rowNum,ang_idx));\n    }\n    \n    if(offer === 'new')\n    {\n      $(\"#analogue_title_form\").hide();\n      $(\"#analogue_supplier_form\").setLabel('Новый поставщик');\n      $(\"#analogue_price_form\").setLabel('Новая цена');\n    }\n    \n    $(\"#analogue_supplier_form\").setValue($(\"#\"+gridId).getText(rowNum,ang_suppl_idx));\n    $(\"#analogue_price_form\").setValue($(\"#\"+gridId).getValue(rowNum,ang_price_idx));\n    $(\"#analogue_tech_check_form\").setValue($(\"#\"+gridId).getValue(rowNum,tech_check_idx));\n    $(\"#ang_tech_check_res_form\").setValue($(\"#\"+gridId).getValue(rowNum,tech_check_res_idx));\n    let techCheckCmt = $(\"#\"+gridId).getValue(rowNum,tech_check_cmt_idx);\n    if(techCheckCmt == null || techCheckCmt.trim() === '')\n    {\n      $(\"#ang_tech_check_cmt_form\").hide();\n    }\n    else\n    {\n      $(\"#ang_tech_check_cmt_form\").show();\n      $(\"#ang_tech_check_cmt_form\").setValue(techCheckCmt);\n    }\n    let url = $(\"#\"+gridId).getValue(rowNum,tech_doc_url_idx);\n    if(url == null || url.trim() == '')\n    {\n      $(\"#ang_tech_url_form\").hide();\n    }\n    else\n    {\n      $(\"#ang_tech_url_form\").show();\n      $(\"#ang_tech_url_form\").setValue(url);\n    }    \n  }\n  else\n  {\n    $(\"#offer_type_form\").setValue('none');\n  }\n}\n\nfunction clearAnalogueFields()\n{\n   $(\"#offer_type_form\").setValue('none');\n   $(\"#analogue_title_form\").setValue('');\n   $(\"#analogue_supplier_form\").setValue('');\n   $(\"#analogue_price_form\").setValue('');\n   $(\"#analogue_tech_check_form\").setValue('none');  \n   $(\"#ang_tech_check_res_form\").setValue('');\n   $(\"#ang_tech_check_cmt_form\").setValue('');\n   $(\"#ang_tech_url_form\").setValue('');\n}\n\nfunction clearSourceItemFields()\n{\n  $(\"#item_code_form\").setValue('');\t  \n  $(\"#item_title_form\").setValue('');\n  $(\"#item_supplier_form\").setValue('');\n  $(\"#item_price_form\").setValue('');\n  $(\"#item_quantity_form\").setValue('');\n  $(\"#item_unit_form\").setValue('');\n}\n"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"Тендерная проверка новых предложений","colSpan":6},{"type":"text","variable":"RequestId","var_uid":"2973547425cb9e404d2e512080167836","dataType":"string","protectedValue":false,"id":"RequestId","name":"RequestId","label":"Номер заказа","defaultValue":"","placeholder":"","hint":"","required":false,"requiredFieldErrorMessage":"","textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"view","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"Reques