        <dyn_title>NewOffersSearch</dyn_title>
        <dyn_description></dyn_description>
        <pro_uid>9810584265cb9d56a295984036422203</pro_uid>
        <dyn_type>xmlform</dyn_type>
        <dyn_filename><![CDATA[9810584265cb9d56a295984036422203/3891327865cb9e3fc5448a6076028379]]></dyn_filename>
        <dyn_content><![CDATA[{"name":"NewOffersSearch","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"3891327865cb9e3fc5448a6076028379","name":"NewOffersSearch","description":"","mode":"edit","script":{"type":"js","code":"$('.title-column').css('white-space','pre-line');\n$('a.form-control').css('color','#2a6496');\n$('a.form-control').css('text-decoration','underline');\nblockAnalogueFields();\nclearAnalogueFields();\nclearSourceItemFields();\nconst selectedRowColor = \"#bebebe\";\nconst mouseOverColor = \"#ededed\";\nconst code_idx = 1;\nconst item_idx = 2;\nconst suppl_idx = 3;\nconst price_idx = 4;\nconst quant_idx = 5;\nconst unit_idx = 6;\nconst ang_idx = 7;\nconst ang_suppl_idx = 8;\nconst ang_price_idx = 9;\nconst tend_doc_idx = 10;\nconst offer_idx = 11;\nconst tech_check_idx = 12;\nconst tender_passed_idx = 13;\nconst suppl_code_idx = 14;\nconst tech_doc_url_idx = 15;\nconst purchase_idx = 17;\nconst tender_requested_idx = 18;\nconst tender_required_idx = 19;\nconst new_suppl_code_idx = 20;\nconst gridId  = \"ItemsList\"; //set to the ID of the grid\nconst formId = '3891327865cb9e3fc5448a6076028379';\n//set click event handler when the DynaForm loads for any existing rows in the grid:\nlet analogueRows = $(\"#\"+gridId).find(\"div.pmdynaform-grid-row\");\nlet currentAngRow = 0;\nlet clickRow = true;\nlet purchaseAllClickFlag = true;\nanalogueRows.click(clickHandler);\nanalogueRows.mouseover(mouseoverHandler);\nanalogueRows.mouseout(mouseoutHandler);\n$(\"#clear_button\").find(\"button\").click(clearClickHandler);\n$(\"#item_save_button\").find(\"button\").click(saveAnalogueClickHandler);\n$(\"#save_form_button\").find(\"button\").click(saveAngFormClick);\n$(\"#purchase_all_btn\").find(\"button\").click(purchaseAllClick);\n$(\"#offer_type_form\").getControl().attr('disabled', true);\n\nlet rowsNumAng = $(\"#\"+gridId).getNumberRows();\n\nfor (var i=1; i <= rowsNumAng; i++)\n{\n  $(\"#\"+gridId).getControl(i, code_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, item_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, suppl_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, price_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, ang_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, ang_price_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, ang_suppl_idx).attr(\"readOnly\", true);\n  $(\"#\"+gridId).getControl(i, ang_idx).attr(\"readOnly\", true);\n  \n  $(\"#\"+gridId).setValue('none',i, tender_passed_idx);\n      \n  var offer = $(\"#\"+gridId).getValue(i,offer_idx);\n  if(offer == null || offer.trim() == '')\n  {\n    $(\"#\"+gridId).setValue('none',i,offer_idx);\n  }\n  \n  var tenderRequired = $(\"#\"+gridId).getValue(i, tender_required_idx);\n  if(tenderRequired === '1')\n  {\n    $(\"#\"+gridId).setValue(1,i, tender_requested_idx);\n  \t$(\"#\"+gridId).getControl(i, tender_requested_idx).attr(\"disabled\", true);\n  }\n}\n\ncheckInput();\nlet requestComment = $('#RequestComment').getValue();\nif(requestComment == null || requestComment.trim() == '')\n{ $('#RequestComment').hide(); }\n\n$(\"#\"+formId).setOnSubmit(function() {\n  \tvar isValid = checkInput();  \n    if(!isValid)\n    {\n      $('#invalidInput').modal('show');\n    }\n  \tvar itemsToPurchase = 0;\n  \tfor (var i=1; i <= rowsNumAng; i++)\n    {\n       let purchase = $(\"#\"+gridId).getValue(i, purchase_idx);\n      if(purchase == '1') itemsToPurchase++;\n    }\n  if(itemsToPurchase == 0)\n  {\n  \tisValid = false;\n     $('#itemsToPurchase').modal('show');\n  }\n    return isValid;\n});\n/*\n * Back to top button\n */\n\n$(\"#back-to-top\").css( \"display\" , \"none\");\n$(\"#back-to-top\").css( \"position\" , \"fixed\");\n$(\"#back-to-top\").css( \"bottom\" , \"42px\");\n$(\"#back-to-top\").css( \"right\" , \"25px\");\n$(\"#back-to-top\").css( \"text-align\" , \"center\");\n$(\"#back-to-top\").css( \"height\" , \"32px\");\n$(\"#back-to-top\").css( \"width\" , \"32px\");\n$(\"#back-to-top\").css( \"cursor\" , \"pointer\");\n$(\"#back-to-top\").css( \"border\" , \"0\");\n$(\"#back-to-top\").css( \"border-radius\" , \"2px\");\n$(\"#back-to-top\").css( \"text-decoration\" , \"none\");\n$(\"#back-to-top\").css( \"padding-left\" , \"9px\");\n$(\"#back-to-top\").click(function(e) {\n            e.preventDefault();\n            $('html, body').animate({\n                scrollTop: 0\n            }, 100);\n       });\n$(window).scroll(function () {\n            if ($(this).scrollTop() > 500) {\n                $('#back-to-top').fadeIn();\n            } else {\n                $('#back-to-top').fadeOut();\n            }\n        });\nfunction checkInput()\n{\n  let validate = true;   \n  let techCheckRequired = false;\n    for (var i=1; i <= rowsNumAng; i++)\n    {\n      $(\"#\"+gridId).getControl(i,ang_suppl_idx).css(\"border\",\"\");\n      $(\"#\"+gridId).getControl(i,ang_price_idx).css(\"border\",\"\");\n      // cheking if supplier or price HAS to be found\n      let suppl = $(\"#\"+gridId).getValue(i,suppl_idx);      \n      let angSuppl = $(\"#\"+gridId).getValue(i,ang_suppl_idx);\n      let purchase =  $(\"#\"+gridId).getValue(i,purchase_idx);\n      if((suppl == null || suppl.trim() === '')\n        && (angSuppl == null || angSuppl.trim() === '') && purchase === 1)\n      {\n         validate = false;\n        $(\"#\"+gridId).getControl(i,ang_suppl_idx).css(\"border\",\"1px solid red\");\n      }\n      \n      let angPrice = $(\"#\"+gridId).getValue(i,ang_price_idx);\n      let price = $(\"#\"+gridId).getValue(i,price_idx);\n      if((price == null || price.trim() === '')\n        &&(angPrice == null || angPrice.trim() === '') && purchase === 1)\n      {\n        validate = false;\n        $(\"#\"+gridId).getControl(i,ang_price_idx).css(\"border\",\"1px solid red\");\n      }\n    }\n    return validate;\n}\nfunction saveAngFormClick()\n{\n  checkInput();\n  $(\"#\"+formId).saveForm()